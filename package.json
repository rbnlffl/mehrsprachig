{
    "name": "mehrsprachig",
    "version": "0.6.0",
    "description": "The simplest way to make your app speak several languages",
    "main": "public/mehrsprachig.cjs.js",
    "module": "public/mehrsprachig.esm.js",
    "unpkg": "public/mehrsprachig.iife.js",
    "license": "MIT",
    "repository": "robinloeffel/mehrsprachig",
    "author": {
        "name": "Robin LÃ¶ffel",
        "email": "hi@robinloeffel.ch",
        "url": "robinloeffel.ch"
    },
    "engines": {
        "node": ">=10"
    },
    "files": [
        "index.js",
        "public/mehrsprachig.*"
    ],
    "keywords": [
        "translate",
        "translations",
        "languages"
    ],
    "dependencies": {
        "core-js": "^3.6.4",
        "regenerator-runtime": "^0.13.5",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/preset-env": "^7.8.7",
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "eslint": "^6.7.2",
        "eslint-config-sweet": "^0.3.0",
        "rollup": "^2.0.6",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-eslint": "^7.0.0",
        "rollup-plugin-terser": "^5.3.0",
        "serve": "^11.3.0"
    },
    "scripts": {
        "clean": "rm -rf public/mehrsprachig.*",
        "build": "yarn clean && rollup -c",
        "watch": "yarn build -w",
        "build:dev": "yarn watch --environment dev",
        "build:prod": "yarn watch",
        "serve": "serve public",
        "dev": "yarn build:dev & yarn serve",
        "prod": "yarn build:prod & yarn serve",
        "release": "yarn clean && yarn build && yarn publish",
        "start": "yarn dev"
    },
    "eslintConfig": {
        "extends": "sweet"
    },
    "browserslist": [
        ">1%",
        "ie 11",
        "not dead"
    ],
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "useBuiltIns": "usage",
                    "corejs": 3
                }
            ]
        ],
        "exclude": "node_modules/**"
    }
}
